<http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO> rdf:type owl:Ontology ;
                                                           <http://purl.org/dc/elements/1.1/creator> "Jaume Mateu Mateu" ;
                                                           <http://purl.org/dc/elements/1.1/title> "FSSO (Flight Satisfaction Survey Ontology)" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/endsAt
:endsAt rdf:type owl:ObjectProperty ;
        rdfs:domain :Trip ;
        rdfs:range :ItineraryPoint ;
        rdfs:comment "A trip ends at an itinerary point." ;
        rdfs:label "endsAt" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/evaluates
:evaluates rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Review ;
           rdfs:range :Flight ;
           rdfs:comment "A review evaluates a flight." ;
           rdfs:label "evaluates" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/evaluatesPositively
:evaluatesPositively rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :evaluates ;
                     rdfs:comment "A review might evaluate a flight positively." ;
                     rdfs:label "evaluatesPositively" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/from
:from rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      rdfs:domain :Flight ;
      rdfs:range :ItineraryPoint ;
      rdfs:comment "A flight starts from an itinerary point." ;
      rdfs:label "from" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/hasBooking
:hasBooking rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Passenger ;
            rdfs:range :Booking ;
            rdfs:comment "A passenger has a booking for a flight." ;
            rdfs:label "hasBooking" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/hasFlight
:hasFlight rdf:type owl:ObjectProperty ;
           rdfs:domain :Trip ;
           rdfs:range :Flight ;
           rdfs:comment "A trip has one or more associated flights." ;
           rdfs:label "hasFlight" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/hasLayover
:hasLayover rdf:type owl:ObjectProperty ;
            rdfs:domain :Trip ;
            rdfs:range :ItineraryPoint ;
            rdfs:comment "A trip might have a layover at an itinerary point." ;
            rdfs:label "hasLayover" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/hasNationality
:hasNationality rdf:type owl:ObjectProperty ;
                rdfs:domain :Passenger ;
                rdfs:range :Country ;
                rdfs:comment "A passenger has a nationality (is from a country)." ;
                rdfs:label "hasNationality" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/hasOnboard
:hasOnboard rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Flight ;
            rdfs:range :Passenger ;
            rdfs:comment "A flight has passengers onboard." ;
            rdfs:label "hasOnboard" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/hasWritten
:hasWritten rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :ReviewAuthor ;
            rdfs:range :Review ;
            rdfs:comment "A review author has written reviews." ;
            rdfs:label "hasWritten" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/includesFlight
:includesFlight rdf:type owl:ObjectProperty ;
                rdfs:domain :Booking ;
                rdfs:range :Flight ;
                rdfs:comment "A booking includes a flight." ;
                rdfs:label "includesFlight" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/includesTrip
:includesTrip rdf:type owl:ObjectProperty ;
              rdfs:domain :Booking ;
              rdfs:range :Trip ;
              rdfs:comment "A booking includes a trip." ;
              rdfs:label "includesTrip" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/isLocatedAt
:isLocatedAt rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :ItineraryPoint ;
             rdfs:range :City ;
             rdfs:comment "An itinerary point is located at some place." ;
             rdfs:label "isLocatedAt" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/measuresQualityOf
:measuresQualityOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   rdfs:domain :Review ;
                   rdfs:range :AirTravelServices ;
                   rdfs:comment "A review measures the quality of air travel services." ;
                   rdfs:label "measuresQualityOf" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/offeredBy
:offeredBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain :Flight ;
           rdfs:range :Airline ;
           rdfs:comment "A flight is offered by an airline." ;
           rdfs:label "offeredBy" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/owns
:owns rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf owl:topObjectProperty ;
      rdfs:domain :Person ;
      rdfs:range :Pet ;
      rdfs:comment "A person owns a pet." ;
      rdfs:label "owns" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/startsAt
:startsAt rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain :Trip ;
          rdfs:range :ItineraryPoint ;
          rdfs:comment "A trip starts at an itinerary point." ;
          rdfs:label "startsAt" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/to
:to rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain :Flight ;
    rdfs:range :ItineraryPoint ;
    rdfs:comment "A flight goes to an itinerary point." ;
    rdfs:label "to" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/usesAircraft
:usesAircraft rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Flight ;
              rdfs:range :Aircraft ;
              rdfs:comment "A flight uses a particular aircraft." ;
              rdfs:label "usesAircraft" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/airlineName
:airlineName rdf:type owl:DatatypeProperty ;
             rdfs:domain :Airline ;
             rdfs:range xsd:string ;
             rdfs:comment "Name of the airline." ;
             rdfs:label "airlineName" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/dateFlown
:dateFlown rdf:type owl:DatatypeProperty ;
           rdfs:domain :Flight ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "Date when a flight flown." ;
           rdfs:label "dateFlown" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/datePublished
:datePublished rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Review ;
               rdfs:range xsd:dateTime ;
               rdfs:comment "Date when a review was published." ;
               rdfs:label "datePublished" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/model
:model rdf:type owl:DatatypeProperty ;
       rdfs:domain :Aircraft ;
       rdfs:range xsd:string ;
       rdfs:comment "Aircraft model." ;
       rdfs:label "model" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/passengerName
:passengerName rdf:type owl:DatatypeProperty ;
               rdfs:domain :Passenger ;
               rdfs:range xsd:string ;
               rdfs:comment "Name of a passenger." ;
               rdfs:label "passengerName" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/placeName
:placeName rdf:type owl:DatatypeProperty ;
           rdfs:domain :Place ;
           rdfs:range xsd:string ;
           rdfs:comment "Name of a place." ;
           rdfs:label "placeName" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/rating
:rating rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdfs:domain :Review ;
        rdfs:range [ rdf:type rdfs:Datatype ;
                     owl:onDatatype xsd:integer ;
                     owl:withRestrictions ( [ xsd:minInclusive "1"^^xsd:int
                                            ]
                                            [ xsd:maxInclusive "10"^^xsd:int
                                            ]
                                          )
                   ] ;
        rdfs:comment "Rating given in a review." ;
        rdfs:label "rating" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/recommended
:recommended rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain :Review ;
             rdfs:range xsd:boolean ;
             rdfs:comment "Whether a flight is recommended or not in a review." ;
             rdfs:label "recommended" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/reviewBody
:reviewBody rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdfs:domain :Review ;
            rdfs:range xsd:string ;
            rdfs:comment "Text of a review." ;
            rdfs:label "reviewBody" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/seatType
:seatType rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :Booking ;
          rdfs:range [ rdf:type rdfs:Datatype ;
                       owl:oneOf [ rdf:type rdf:List ;
                                   rdf:first "Business Class" ;
                                   rdf:rest [ rdf:type rdf:List ;
                                              rdf:first "Economy Class" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "First Class" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "Premium Economy" ;
                                                                    rdf:rest rdf:nil
                                                                  ]
                                                       ]
                                            ]
                                 ]
                     ] ;
          rdfs:comment "Type of seat." ;
          rdfs:label "seatType" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/title
:title rdf:type owl:DatatypeProperty ;
       rdfs:domain :Review ;
       rdfs:range xsd:string ;
       rdfs:comment "Title of a review." ;
       rdfs:label "title" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/verified
:verified rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf owl:topDataProperty ;
          rdfs:domain :Review ;
          rdfs:range xsd:boolean ;
          rdfs:comment "Whether a review was verified or not." ;
          rdfs:label "verified" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/AirTravelServices
:AirTravelServices rdf:type owl:Class ;
                   rdfs:comment "Services provided to passengers related to flying, including both ground and in-flight amenities." ;
                   rdfs:label "Air travel services" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Aircraft
:Aircraft rdf:type owl:Class ;
          rdfs:comment "Vehicle that can fly through the air, carrying passengers, cargo, or for other purposes." ;
          rdfs:label "Aircraft" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Airline
:Airline rdf:type owl:Class ;
         rdfs:comment "Company that provides air transport services for traveling passengers and freight." ;
         rdfs:label "Airline" ;
         rdfs:sameAs "https://dbpedia.org/ontology/Airline" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Booking
:Booking rdf:type owl:Class ;
         rdfs:comment "Reservation for a flight or trip made by a passenger." ;
         rdfs:label "Booking" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/CabinStaffServices
:CabinStaffServices rdf:type owl:Class ;
                    rdfs:subClassOf :AirTravelServices ;
                    rdfs:comment "Assistance and care services provided by flight attendants to passengers during a flight." ;
                    rdfs:label "Cabin staff services" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Place ;
      rdfs:comment "Large and densely populated urban area." ;
      rdfs:label "City" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf :Place ;
         rdfs:comment "Political entity recognized as an independent nation, often characterized by a common government and boundaries that define its geographical limits" ;
         rdfs:label "Country" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Flight
:Flight rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :from ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :ItineraryPoint
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :to ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :ItineraryPoint
                        ] ;
        rdfs:comment "Journey made by an aircraft from one airport to another, including all stages of departure, travel, and arrival." ;
        rdfs:label "Flight" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/FoodAndBeverages
:FoodAndBeverages rdf:type owl:Class ;
                  rdfs:subClassOf :AirTravelServices ;
                  rdfs:comment "Services of provision of meals and drinks to passengers during a flight." ;
                  rdfs:label "Food and beverages services" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/GroundService
:GroundService rdf:type owl:Class ;
               rdfs:subClassOf :AirTravelServices ;
               rdfs:comment "Airport-based support services, including check-in, baggage handling, and passenger assistance." ;
               rdfs:label "Ground services" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/ItineraryPoint
:ItineraryPoint rdf:type owl:Class ;
                rdfs:comment "Specific location that is a designated stop or destination along a travel route, as outlined in a travel itinerary." ;
                rdfs:label "Itinerary Point" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Passenger
:Passenger rdf:type owl:Class ;
           rdfs:comment "Idividual or animal that travels in a vehicle specifically for the purpose of transportation, without being part of the operating or service crew." ;
           rdfs:label "Passenger" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf :Passenger ;
        rdfs:comment "Individual human who travels in a vehicle for the purpose of transportation, without being involved in operating the vehicle or providing onboard services." ;
        rdfs:label "Person" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Pet
:Pet rdf:type owl:Class ;
     rdfs:subClassOf :Passenger ;
     rdfs:comment "Domesticated animal kept for companionship or pleasure, which may accompany its owner or a designated handler during travel." ;
     rdfs:label "Pet" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Place
:Place rdf:type owl:Class ;
       rdfs:comment "Specific location or area, defined by geographical, cultural, or functional characteristics." ;
       rdfs:label "Place" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Region
:Region rdf:type owl:Class ;
        rdfs:subClassOf :Place ;
        rdfs:comment "Area distinguished by unique, administrative characteristics, larger than a single location but smaller than a country." ;
        rdfs:label "Region" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Review
:Review rdf:type owl:Class ;
        rdfs:comment "Evaluation provided by a customer or user regarding their experience with a product, service, or establishment, often shared publicly to inform others or provide feedback to the provider." ;
        rdfs:label "Review" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/ReviewAuthor
:ReviewAuthor rdf:type owl:Class ;
              rdfs:subClassOf :Person ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :hasWritten ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Review
                              ] ;
              rdfs:comment "An author of a review." ;
              rdfs:label "ReviewAuthor" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/Trip
:Trip rdf:type owl:Class ;
      rdfs:comment "Journey or voyage made from one place to another, often for a specific purpose or pleasure." ;
      rdfs:label "Trip" .


###  http://www.semanticweb.org/jamat/ontologies/2024/3/FSSO/WifiAndConnectivity
:WifiAndConnectivity rdf:type owl:Class ;
                     rdfs:subClassOf :AirTravelServices ;
                     rdfs:comment "Services of internet access and communication facilities aboard a flight." ;
                     rdfs:label "Wifi and Connectivtiy Service" .


